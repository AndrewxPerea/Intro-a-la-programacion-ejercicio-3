package org.example;

public class Main {

    public static void main(String[] args) {
        int numeroIf = 3;

        if ( numeroIf >= 1 ){
            System.out.println("positivo");
        }
        else if (numeroIf == 0) {
            System.out.println("0");
        }
        else
            System.out.println("negativo");
    }

}
