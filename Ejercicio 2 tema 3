package org.example;
//ejericio 2
public class Main {

    public static void main(String[] args) {
        Coche miCoche = new  Coche();
        miCoche.AumentoPuertas();
        miCoche.AumentoPuertas();
        System.out.println(miCoche.puertas);
    }
}
class Coche {
    public int puertas = 0;

    public void AumentoPuertas() {
        this.puertas++;
    }
}
